    <TextView
        android:id="@+id/text_temp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="16dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:textColor="@color/colorTextPrimary"
        android:textSize="@dimen/small_icon_text_size"
        app:fontFamily="sans-serif-light"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/image_temp"
        app:layout_constraintStart_toStartOf="@+id/image_temp"
        app:layout_constraintTop_toBottomOf="@+id/image_temp" />

    <ImageView
        android:id="@+id/image_power"
        android:layout_width="@dimen/medium_icon_size"
        android:layout_height="@dimen/medium_icon_size"
        android:tint="@android:color/darker_gray"
        app:layout_constraintBottom_toBottomOf="@+id/image_online"
        app:layout_constraintEnd_toStartOf="@+id/image_online"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/image_security"
        app:layout_constraintTop_toTopOf="@+id/image_online"
        app:srcCompat="@drawable/ic_power" />

    <ImageView
        android:id="@+id/image_notify"
        android:layout_width="@dimen/medium_icon_size"
        android:layout_height="@dimen/medium_icon_size"
        android:layout_marginTop="16dp"
        android:tint="@android:color/darker_gray"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/image_temp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_notifications_active" />

    <ImageView
        android:id="@+id/image_temp"
        android:layout_width="@dimen/medium_icon_size"
        android:layout_height="@dimen/medium_icon_size"
        android:tint="@android:color/darker_gray"
        app:layout_constraintBottom_toBottomOf="@+id/image_notify"
        app:layout_constraintEnd_toStartOf="@+id/image_notify"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/image_online"
        app:layout_constraintTop_toTopOf="@+id/image_notify"
        app:srcCompat="@drawable/ic_temp" />

    <ImageView
        android:id="@+id/image_online"
        android:layout_width="@dimen/medium_icon_size"
        android:layout_height="@dimen/medium_icon_size"
        android:tint="@android:color/darker_gray"
        app:layout_constraintBottom_toBottomOf="@+id/image_temp"
        app:layout_constraintEnd_toStartOf="@+id/image_temp"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/image_power"
        app:layout_constraintTop_toTopOf="@+id/image_temp"
        app:srcCompat="@drawable/ic_online" />

    <ImageView
        android:id="@+id/image_security"
        android:layout_width="@dimen/medium_icon_size"
        android:layout_height="@dimen/medium_icon_size"
        android:tint="@android:color/darker_gray"
        app:layout_constraintBottom_toBottomOf="@+id/image_power"
        app:layout_constraintEnd_toStartOf="@+id/image_power"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/image_power"
        app:srcCompat="@drawable/ic_security" />

    <TextView
        android:id="@+id/text_power"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:textColor="@color/colorTextPrimary"
        android:textSize="@dimen/small_icon_text_size"
        app:fontFamily="sans-serif-light"
        app:layout_constraintBottom_toBottomOf="@+id/text_temp"
        app:layout_constraintEnd_toEndOf="@+id/image_power"
        app:layout_constraintStart_toStartOf="@+id/image_power"
        app:layout_constraintTop_toTopOf="@+id/text_temp" />

    <TextView
        android:id="@+id/text_online"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:textColor="@color/colorTextPrimary"
        android:textSize="@dimen/small_icon_text_size"
        app:fontFamily="sans-serif-light"
        app:layout_constraintBottom_toBottomOf="@+id/text_temp"
        app:layout_constraintEnd_toEndOf="@+id/image_online"
        app:layout_constraintStart_toStartOf="@+id/image_online"
        app:layout_constraintTop_toTopOf="@+id/text_temp" />

    <TextView
        android:id="@+id/text_security"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:textColor="@color/colorTextPrimary"
        android:textSize="@dimen/small_icon_text_size"
        app:fontFamily="sans-serif-light"
        app:layout_constraintBottom_toBottomOf="@+id/text_temp"
        app:layout_constraintEnd_toEndOf="@+id/image_security"
        app:layout_constraintStart_toStartOf="@+id/image_security"
        app:layout_constraintTop_toTopOf="@+id/text_temp" />

    <TextView
        android:id="@+id/text_notify"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:textColor="@color/colorTextPrimary"
        android:textSize="@dimen/small_icon_text_size"
        app:fontFamily="sans-serif-light"
        app:layout_constraintBottom_toBottomOf="@+id/text_temp"
        app:layout_constraintEnd_toEndOf="@+id/image_notify"
        app:layout_constraintStart_toStartOf="@+id/image_notify"
        app:layout_constraintTop_toTopOf="@+id/text_temp" />

